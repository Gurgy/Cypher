<HBox xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.cypher.gui.root.roomcollection.RoomCollectionPresenter">
    <children>
        <AnchorPane maxHeight="1.7976931348623157E308" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="184.0">
            <children>
                <Label layoutX="10.0" layoutY="10.0" prefHeight="27.0" prefWidth="180.0" styleClass="h3" text="Server Name" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0" />
                <Button layoutX="156.0" layoutY="37.0" mnemonicParsing="false" onMouseClicked="#showDirectory" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="37.0">
                    <styleClass>
                        <String fx:value="btn" />
                        <String fx:value="btn-info" />
                        <String fx:value="image-btn" />
                    </styleClass>
                    <graphic>
                        <FontIcon iconColor="WHITE" iconLiteral="fa-folder-open" iconSize="20" />
                    </graphic></Button>
                <ListView fx:id="roomList" layoutY="73.0" styleClass="room-list" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="73.0" />
            </children>
        </AnchorPane>
        <StackPane fx:id="rightSideStackPane" HBox.hgrow="ALWAYS" />
    </children>
</HBox>
