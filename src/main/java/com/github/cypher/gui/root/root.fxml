<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.*?>
<?import org.kordamp.ikonli.javafx.FontIcon?>
<?import java.lang.*?>
<StackPane fx:id="mainStackPane" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.cypher.gui.root.RootPresenter">
    <children>
        <HBox>
            <children>
                <AnchorPane minHeight="300.0" minWidth="-Infinity">
                    <children>
                        <VBox layoutX="5.0" layoutY="14.0" AnchorPane.bottomAnchor="135.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="14.0">
                            <ListView fx:id="roomCollectionListView" focusTraversable="false" prefHeight="0.0" prefWidth="76.0" styleClass="server-list">
                                <VBox.margin>
                                    <Insets />
                                </VBox.margin>
                            </ListView>
                            <Button mnemonicParsing="false" onAction="#onAddButtonAction" prefHeight="60.0" prefWidth="60.0">
                                <styleClass>
                                    <String fx:value="btn" />
                                    <String fx:value="btn-info" />
                                    <String fx:value="image-btn" />
                                </styleClass>
                                <graphic>
                                    <FontIcon iconColor="WHITE" iconLiteral="fa-plus" iconSize="40" text="" />
                                </graphic>
                            </Button>
                        </VBox>
                        <Button fx:id="logoutButton" layoutX="13.0" layoutY="370.0" mnemonicParsing="false" onAction="#logout" prefHeight="60.0" prefWidth="60.0" style="-fx-min-width: 2px;" wrapText="true" AnchorPane.bottomAnchor="70.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="21.0">
                            <graphic>
                                <FontIcon iconColor="WHITE" iconLiteral="fa-angle-left" iconSize="40" textAlignment="CENTER" />
                            </graphic>
                            <styleClass>
                                <String fx:value="btn-info" />
                                <String fx:value="btn" />
                                <String fx:value="image-btn" />
                            </styleClass>
                        </Button>
                        <Button layoutX="13.0" layoutY="443.0" mnemonicParsing="false" onAction="#toggleSettings" prefHeight="60.0" prefWidth="60.0" style="-fx-min-width: 2px;" wrapText="true" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="21.0">
                            <graphic>
                                <FontIcon iconColor="WHITE" iconLiteral="fa-cog" iconSize="40" textAlignment="CENTER" />
                            </graphic>
                            <styleClass>
                                <String fx:value="btn-info" />
                                <String fx:value="btn" />
                                <String fx:value="image-btn" />
                            </styleClass>
                        </Button>
                    </children>
                </AnchorPane>
                <StackPane fx:id="rightSideStackPane" HBox.hgrow="ALWAYS" />
            </children>
        </HBox>
    </children>
</StackPane>
